
<h1><%=@location.location_name %></h1>
<h4><%= @location.address%></h4>

    

<div class="container">
<div class="jumbotron">
  
    <div class="form-group">





  <% if current_user.id == @location.user_id %>
    <H2 ><%= link_to "Delete", location_path(@location), method: :delete, data: { confirm: 'Are you certain you want to delete this?' }
 %></H2>
  <% end %>

</div>
</div>
</div>